    "generative-openai": {
      "model": "gpt-3.5-turbo",
      "temperatureProperty": 0.2,
      "maxTokensProperty": 512
    },
    "qna-openai": {
      "model": "text-davinci-002",
      "maxTokens": 2500,
      "temperature": 0.0
    }
  }
