{
  "class": "MedicalDocs",
  "vectorizer": "text2vec-huggingface",
  "properties": [
    {
      "dataType": [
        "text"
      ],
      "name": "url"
    },
    {
      "dataType": [
        "text"
      ],
      "name": "crawl_date"
    },
    {
      "dataType": [
        "text"
      ],
      "name": "en"
    },
    {
      "dataType": [
        "text"
      ],
      "name": "vi"
    }
  ],
  "moduleConfig": {
    "text2vec-huggingface": {
      "model": "sentence-transformers/all-MiniLM-L6-v2",
      "options": {
        "waitForModel": true
      }
